<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MuridAFActivity">

    <TextView
        android:id="@+id/Nama"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Nama"
        android:textSize="12pt"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="55dp" />

    <TextView
        android:id="@+id/Alamat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:text="Alamat"
        android:textSize="12pt"
        app:layout_constraintBottom_toTopOf="@+id/Nama2"
        app:layout_constraintTop_toBottomOf="@+id/Nama"
        tools:layout_editor_absoluteX="16dp" />


    <TextView
        android:id="@+id/Nama2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:text="Nama"
        android:textSize="12pt"
        app:layout_constraintBottom_toTopOf="@+id/Alamat2"
        app:layout_constraintTop_toBottomOf="@+id/Alamat"
        tools:layout_editor_absoluteX="-38dp" />

    <TextView
        android:id="@+id/Alamat2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Alamat"
        android:textSize="12pt"
        app:layout_constraintTop_toBottomOf="@+id/Nama2"
        tools:layout_editor_absoluteX="-78dp" />

    <TextView
        android:id="@+id/Nama3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Nama"
        android:textSize="12pt"
        app:layout_constraintTop_toBottomOf="@+id/Alamat2"
        tools:layout_editor_absoluteX="40dp" />

    <TextView
        android:id="@+id/Alamat3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Alamat"
        android:textSize="12pt"
        app:layout_constraintTop_toBottomOf="@+id/Nama3"
        tools:layout_editor_absoluteX="-31dp" />



</androidx.constraintlayout.widget.ConstraintLayout>